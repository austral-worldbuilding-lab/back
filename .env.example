DATABASE_URL="postgresql://postgres:password@localhost:5432/postgres?schema=public"
FIREBASE_PROJECT_ID="austral-worldbuilding-lab"
FIREBASE_CLIENT_EMAIL="client-email"
FIREBASE_PRIVATE_KEY="private-key"
AZURE_STORAGE_ACCOUNT="account"
AZURE_STORAGE_ACCESS_KEY="access-key"
AZURE_STORAGE_CONTAINER_NAME="container-name"
GEMINI_MODEL=model
GEMINI_API_KEY=api-key

# AI Generation Limits
AI_MAX_RESULTS_PER_REQUEST=24
AI_MIN_RESULTS_PER_REQUEST=6
AI_MIN_PROVOCATIONS_PER_REQUEST=3
AI_MAX_PROVOCATIONS_PER_REQUEST=5
PROJECT_MIN_MANDALAS_FOR_PROVOCATIONS=5
PROJECT_MIN_POSTITS_FOR_PROVOCATIONS=50
PROJECT_MIN_FILES_FOR_PROVOCATIONS=15
AI_MIN_POSTITS_PER_REQUEST=6
AI_MAX_POSTITS_PER_REQUEST=24
AI_MIN_QUESTIONS_PER_REQUEST=6
AI_MAX_QUESTIONS_PER_REQUEST=24

# AI Temperature Configuration (range: 0.0 to 1.0)
# Higher values (0.7-1.0) = more creative/random responses
# Lower values (0.0-0.3) = more deterministic/consistent responses
# If not set, default values are used (see ai-temperature.config.ts)
AI_TEMPERATURE_POSTITS=0.7
AI_TEMPERATURE_CONTEXT_POSTITS=0.7
AI_TEMPERATURE_QUESTIONS=0.6
AI_TEMPERATURE_PROVOCATIONS=0.9
AI_TEMPERATURE_ENCYCLOPEDIA=0.3
AI_TEMPERATURE_MANDALA_SUMMARY=0.4
AI_TEMPERATURE_POSTITS_SUMMARY=0.4
AI_TEMPERATURE_SOLUTIONS=0.7

NODE_ENV=development
# NODE_ENV=production
PRETTY_LOGS=true
# PRETTY_LOGS=false

# Mail settings
MAIL_HOST=mail.example.com
MAIL_PORT=587
MAIL_USER=user@example.com
MAIL_PASSWORD=password
MAIL_FROM=from@example.com
FRONTEND_BASE_URL=http://localhost:5173

# Throttling Configuration
THROTTLER_ENABLED=true
RATE_LIMIT_TTL=60000
RATE_LIMIT_LIMIT=250
