generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Define your models here
model User {
  id    String @id @default(uuid())
  name  String
  email String @unique
}

model Project {
  id          String   @id @default(uuid())
  name        String
  createdAt   DateTime @default(now())
  createdById String
}